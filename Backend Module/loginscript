CREATE DATABASE 'seculog';
CREATE USER 'lnsys'@'localhost' IDENTIFIED BY 'rJ!DiC-i9JqfC&6t0Gm8HCfJUUtNs4O^';
GRANT SELECT, INSERT, UPDATE ON 'seculog' .* TO 'lnsys'@'localhost';
CREATE TABLE 'seculog' . 'members' (
	'id' INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
	'username' VARCHAR(62) NOT NULL,
	'email' VARCHAR(62) NOT NULL
	'password' CHAR(128) NULL NULL
) ENGINE = InnoDB;
CREATE TABLE 'seculog'.'login_attempts' (
	'user_id' INT(11) NOT NULL,
	'time' VARCHAR(30) NOT NULL
) ENGINE=InnoDB
